<h1> Sign Up For a New Account </h1>
<br>
  <%= form_for @user, url: { action: "create" } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <p><br>
  <%= f.label :picture %>
  <%= f.file_field :avatar %>
  <p><br>
  <%= f.submit %>
<% end %>
